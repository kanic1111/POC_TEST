package com.test.poc_test;

import com.test.poc_test.POC2_1;

rule "POC passed"
    when
        $POC : POC2_1(ap_price > 3000 && level < 5 && ap_price_percent > 0.8 && mp_max_price < 1000)
    then
        $POC.setStatus(true);
        // update($s);
end

rule "POC failed"
    when
        $POC : POC2_1(ap_price <= 3000 || level > 5 || ap_price_percent <= 0.8 || mp_max_price >= 1000)
    then
        $POC.setStatus(false);
        // update($s);
end